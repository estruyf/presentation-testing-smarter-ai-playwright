{
  "demoTime.defaultFileType": "yaml",
  "demoTime.customTheme": ".demo/theme/mc2mc.css",
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "workbench.statusBar.visible": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.activityBar.location": "default",
  "demoTime.highlightBorderColor": "transparent",
  "demoTime.highlightBackground": "rgba(19, 142, 151, 0.2)",
  "demoTime.highlightOpacity": 0.5,
  "demoTime.highlightBlur": 2,
  "editor.hover.enabled": "off",
  "demoTime.highlightZoomEnabled": 5,
  "demoTime.customWebComponents": [
    ".demo/components/loading.js"
  ],
  "workbench.colorTheme": "Demo Time Light (no italic)"
}